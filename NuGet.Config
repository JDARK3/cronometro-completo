<?xml version="1.0" encoding="utf-8"?>
<!-- 
  Arquivo de configuração do NuGet para gerenciamento de pacotes
  Define as fontes de pacotes disponíveis para restauração
-->
<configuration>
  <!-- Seção que define as fontes de pacotes NuGet -->
  <packageSources>
    <!-- 
      Adiciona o repositório oficial nuget.org como fonte de pacotes
      - key: Identificador único para a fonte
      - value: URL do serviço de índice do NuGet (v3 API)
    -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
</configuration>